<?xml version="1.0" encoding="utf-8"?>
<DependentLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_content"
    ohos:width="match_parent"
>

    <StackLayout
        ohos:id="$+id:app_bar"
        ohos:height="match_content"
        ohos:width="match_parent"
    >

        <DirectionalLayout
            ohos:id="$+id:title_layout"
            ohos:height="$float:title_layout_height"
            ohos:width="match_parent"
            ohos:alignment="left|bottom"
            ohos:background_element="$color:title_layout_background_color"
            ohos:orientation="vertical"
        >

            <Text
                ohos:id="$+id:title"
                ohos:height="$float:title_text_background_height"
                ohos:width="match_content"
                ohos:bottom_margin="$float:title_text_bottom_margin"
                ohos:bottom_padding="$float:title_text_bottom_padding"
                ohos:component_description="$string:title_item_description"
                ohos:left_padding="$float:title_text_left_padding"
                ohos:text="$string:title_text"
                ohos:text_alignment="vertical_center"
                ohos:text_color="$color:title_text_color"
                ohos:text_size="$float:title_text_size"
                ohos:top_padding="$float:title_text_top_padding"
            />

        </DirectionalLayout>

        <DirectionalLayout
            ohos:id="$+id:head_button_layout"
            ohos:height="$float:head_button_layout_height"
            ohos:width="match_parent"
            ohos:alignment="vertical_center|right"
            ohos:orientation="horizontal"
            ohos:right_margin="$float:head_button_layout_right_margin"
        >

            <DirectionalLayout
                ohos:id="$+id:first_item_layout"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alignment="center"
                ohos:component_description="$string:top_first_item_description"
                ohos:left_margin="$float:head_button_left_margin"
                ohos:orientation="horizontal"
                ohos:right_margin="$float:head_button_right_margin"
            >

                <Image
                    ohos:id="$+id:first_item"
                    ohos:height="$float:head_button_image_height"
                    ohos:width="$float:head_button_image_width"
                    ohos:alpha="0.9"
                    ohos:foreground_element="$media:add"
                />

            </DirectionalLayout>

            <DirectionalLayout
                ohos:id="$+id:second_item_layout"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alignment="center"
                ohos:component_description="$string:top_second_item_description"
                ohos:left_margin="$float:head_button_left_margin"
                ohos:orientation="horizontal"
                ohos:right_margin="$float:head_button_right_margin"
            >

                <Image
                    ohos:id="$+id:second_item"
                    ohos:height="$float:head_button_image_height"
                    ohos:width="$float:head_button_image_width"
                    ohos:alpha="0.9"
                    ohos:foreground_element="$media:add"
                />
            </DirectionalLayout>

            <DirectionalLayout
                ohos:id="$+id:third_item_layout"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alignment="center"
                ohos:component_description="$string:top_third_item_description"
                ohos:left_margin="$float:head_button_left_margin"
                ohos:orientation="horizontal"
                ohos:right_margin="$float:head_button_right_margin"
            >

                <Image
                    ohos:id="$+id:third_item"
                    ohos:height="$float:head_button_image_height"
                    ohos:width="$float:head_button_image_width"
                    ohos:alpha="0.9"
                    ohos:foreground_element="$media:more"
                />
            </DirectionalLayout>

        </DirectionalLayout>

    </StackLayout>

    <DirectionalLayout
        ohos:id="$+id:list_layout"
        ohos:height="match_content"
        ohos:width="match_parent"
        ohos:below="$id:app_bar"
        ohos:bottom_margin="$float:list_layout_bottom_margin"
        ohos:left_margin="$float:list_layout_left_margin"
        ohos:orientation="vertical"
        ohos:right_margin="$float:list_layout_right_margin"
    >

        <ListContainer
            ohos:id="$+id:list_container"
            ohos:height="match_content"
            ohos:width="match_parent"
            ohos:long_click_enabled="false"
        />

    </DirectionalLayout>

    <DirectionalLayout
        ohos:id="$+id:bottom_bar_layout"
        ohos:height="$float:bottom_bar_height"
        ohos:width="match_parent"
        ohos:align_parent_bottom="true"
        ohos:alignment="vertical_center"
        ohos:background_element="$color:bottom_bar_background_color"
        ohos:orientation="horizontal"
    >

        <DirectionalLayout
            ohos:id="$+id:bottom_item_layout1"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:alignment="horizontal_center"
            ohos:orientation="vertical"
            ohos:visibility="hide"
            ohos:weight="1"
        >

            <Image
                ohos:id="$+id:bottom_item_image"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.9"
                ohos:image_src="$media:ori_star"
                ohos:scale_x="1"
                ohos:scale_y="1"
            />

            <Text
                ohos:id="$+id:bottom_item_text"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.95"
                ohos:text="Operation1"
                ohos:text_size="$float:bottom_item_text_size"
            />

        </DirectionalLayout>

        <DirectionalLayout
            ohos:id="$+id:bottom_item_layout2"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:alignment="horizontal_center"
            ohos:orientation="vertical"
            ohos:visibility="hide"
            ohos:weight="1"
        >

            <Image
                ohos:id="$+id:bottom_item_image"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.9"
                ohos:image_src="$media:ori_star"
                ohos:scale_x="1"
                ohos:scale_y="1"
            />

            <Text
                ohos:id="$+id:bottom_item_text"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.95"
                ohos:text="Operation2"
                ohos:text_size="$float:bottom_item_text_size"
            />

        </DirectionalLayout>

        <DirectionalLayout
            ohos:id="$+id:bottom_item_layout3"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:alignment="horizontal_center"
            ohos:orientation="vertical"
            ohos:visibility="hide"
            ohos:weight="1"
        >

            <Image
                ohos:id="$+id:bottom_item_image"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.9"
                ohos:image_src="$media:ori_star"
                ohos:scale_x="1"
                ohos:scale_y="1"
            />

            <Text
                ohos:id="$+id:bottom_item_text"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.95"
                ohos:text="Operation3"
                ohos:text_size="$float:bottom_item_text_size"
            />

        </DirectionalLayout>

        <DirectionalLayout
            ohos:id="$+id:bottom_item_layout4"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:alignment="horizontal_center"
            ohos:orientation="vertical"
            ohos:visibility="hide"
            ohos:weight="1"
        >

            <Image
                ohos:id="$+id:bottom_item_image"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.9"
                ohos:image_src="$media:ori_star"
                ohos:scale_x="1"
                ohos:scale_y="1"
            />

            <Text
                ohos:id="$+id:bottom_item_text"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.95"
                ohos:text="Operation4"
                ohos:text_size="$float:bottom_item_text_size"
            />

        </DirectionalLayout>

        <DirectionalLayout
            ohos:id="$+id:bottom_item_layout5"
            ohos:height="match_content"
            ohos:width="match_content"
            ohos:alignment="horizontal_center"
            ohos:orientation="vertical"
            ohos:visibility="hide"
            ohos:weight="1"
        >

            <Image
                ohos:id="$+id:bottom_item_image"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.9"
                ohos:image_src="$media:ori_star"
                ohos:scale_x="1"
                ohos:scale_y="1"
            />

            <Text
                ohos:id="$+id:bottom_item_text"
                ohos:height="match_content"
                ohos:width="match_content"
                ohos:alpha="0.95"
                ohos:text="Operation5"
                ohos:text_size="$float:bottom_item_text_size"
            />

        </DirectionalLayout>

    </DirectionalLayout>

</DependentLayout>